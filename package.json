{
  "name": "@deepstream/storage-rethinkdb",
  "version": "2.0.0",
  "description": "Connects deepstream.io to rethinkdb",
  "main": "dist/src/connector.js",
  "scripts": {
    "tsc": "tsc",
    "lint": "tslint --project .",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha --opts mocha.opts 'test/*.spec.ts'",
    "coverage": "nyc mocha --exit",
    "ci": "npm run tsc && npm run coverage",
    "docker": "docker run -p 28015:28015 rethinkdb:2.3.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deepstreamIO/deepstream.io-storage-rethinkdb.git"
  },
  "author": "deepstreamHub GmbH",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/deepstreamIO/deepstream.io-storage-rethinkdb/issues"
  },
  "homepage": "http://deepstream.io",
  "dependencies": {
    "@deepstream/types": "^1.0.0",
    "rethinkdb": "2.3.3"
  },
  "devDependencies": {
    "@deepstream/protobuf": "^1.0.0",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/rethinkdb": "^2.3.14",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "mocha": "^6.2.0",
    "nyc": "^13.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "reporter": [
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
